<SolverOptimization>
    <parameter ref="tau" name="tau" from="-3.0" to="-0.9" step="0.1" type="log10" initial="-2" />
    <parameter ref="q" name="q" initial="0">
        <value>0</value>
        <value>1</value>
        <value>2</value>
        <value>3</value>
    </parameter>

    <fcbiilu2 enabled="true">
        <test optimizer="alternating" enabled="true" verbosity="3" buffer="15">
            <parameter ref="tau" />
            <parameter ref="q" />
        </test>
    </fcbiilu2>
</SolverOptimization>